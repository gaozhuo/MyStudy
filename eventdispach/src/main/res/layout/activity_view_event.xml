<?xml version="1.0" encoding="utf-8"?>
<com.gaozhuo.eventdispach.TestLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mylayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">


    <com.gaozhuo.eventdispach.TestButton
        android:id="@+id/my_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="click test" />

    <RelativeLayout
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="#ff8709">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="hello world"
            android:layout_centerInParent="true"/>

    </RelativeLayout>


</com.gaozhuo.eventdispach.TestLinearLayout>